{
  "name": "mp-inject",
  "version": "3.0.0",
  "description": "A type-based dependency injection library.",
  "main": "index.js",
  "scripts": {
    "test": "mocha --require ts-node/register test/**/*.{ts,tsx}",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "fix": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check --watch",
    "build": "npm run type-check&&npm run fix&&webpack",
    "pack": "npm run build&&npm pack"
  },
  "keywords": [
    "miniprogram",
    "DI",
    "dependency injection"
  ],
  "author": "fisher <i.yu@qq.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/fishen/mp-inject.git"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.3",
    "bundle-dts": "^1.1.4",
    "chai": "^4.2.0",
    "mocha": "^8.1.3",
    "ts-loader": "^8.0.4",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.0.1"
  }
}
