{
  "name": "mp-inject",
  "version": "2.6.0",
  "description": "A simple dependency injection library compatible with mini-programs.",
  "main": "index.js",
  "scripts": {
    "test": "mocha --require ts-node/register test/**/*.{ts,tsx}",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "fix": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run type-check&&npm run fix&&webpack",
    "pack": "npm run build&&npm pack"
  },
  "keywords": [
    "miniprogram",
    "DI",
    "dependency injection",
    "wxapp",
    "tt",
    "swan",
    "my",
    "taro"
  ],
  "author": "fisher <i.yu@qq.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/fishen/mp-inject.git"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.0",
    "bundle-dts": "^1.1.3",
    "chai": "^4.2.0",
    "mocha": "^8.0.1",
    "ts-loader": "^8.0.1",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^2.5.0"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.0.0"
  }
}